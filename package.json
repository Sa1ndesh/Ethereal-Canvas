{
  "name": "ethereal-canvas",
  "description": "Cross-Platform AI Art Studio - Transform your imagination into stunning AI-generated artwork and mint them as unique NFTs",
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "ai",
    "art",
    "nft",
    "blockchain",
    "react",
    "typescript",
    "android",
    "web3",
    "capacitor"
  ],
  "author": "Sa1ndesh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sa1ndesh/Ethereal-Canvas.git"
  },
  "homepage": "https://github.com/Sa1ndesh/Ethereal-Canvas",
  "bugs": {
    "url": "https://github.com/Sa1ndesh/Ethereal-Canvas/issues"
  },
  "scripts": {
    "dev": "vite --host",
    "dev:fast": "vite --host --force",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf node_modules/.vite dist",
    "compile": "hardhat compile",
    "deploy:local": "hardhat run scripts/deploy.cjs --network hardhat",
    "deploy:sepolia": "hardhat run scripts/deploy.cjs --network sepolia",
    "deploy:mainnet": "hardhat run scripts/deploy.cjs --network mainnet",
    "verify": "hardhat verify --network sepolia"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.2",
    "@capacitor/status-bar": "^7.0.2",
    "@faker-js/faker": "^9.9.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.9.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.511.0",
    "nft.storage": "^7.2.0",
    "openai": "^5.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.8.2",
    "replicate": "^1.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@eslint/js": "^9.27.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^4.9.3",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "connect-history-api-fallback": "^2.0.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "hardhat": "^2.19.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
